  "description": "Flowpay.js",
  "main": "dist/flowpay.js",
  "module": "dist/flowpay.esm.js",
  "jsnext:main": "dist/flowpay.esm.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "scripts": {
