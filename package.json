    "dist",
    "src",
    "types"
  ],
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.5.0",
    "conditional-type-checks": "^1.0.6",
    "eslint": "^8.43.0",
