    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3",
    "zx": "^7.2.2"
